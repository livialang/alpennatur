############
### PAGE ###
############
page {
    logo {
        file = EXT:alpennatur/Resources/Public/Images/alpennatur-grün.jpg
        fileInverted = EXT:alpennatur/Resources/Public/Images/alpennatur-grün.jpg
        height = 52
        width = 192
        alt = alpennatur
        linktitle = alpennatur
    }
    favicon {
        file = EXT:alpennatur/Resources/Public/Icons/alpennatur-grün.jpg
    }
    fluidtemplate {
        layoutRootPath = EXT:alpennatur/Resources/Private/Layouts/Page/
        partialRootPath = EXT:alpennatur/Resources/Private/Partials/Page/
        templateRootPath = EXT:alpennatur/Resources/Private/Templates/Page/
    }
}


##################
### EXTENSIONS ###
##################
plugin.bootstrap_package {
    view {
        layoutRootPath = EXT:alpennatur/Resources/Private/Layouts/
        partialRootPath = EXT:alpennatur/Resources/Private/Partials/
        templateRootPath = EXT:alpennatur/Resources/Private/Templates/
    }
}


########################
### CONTENT ELEMENTS ###
########################
plugin.bootstrap_package_contentelements {
    view {
        layoutRootPath = EXT:alpennatur/Resources/Private/Layouts/ContentElements/
        partialRootPath = EXT:alpennatur/Resources/Private/Partials/ContentElements/
        templateRootPath = EXT:alpennatur/Resources/Private/Templates/ContentElements/
    }
}


################################
### BOOTSTRAP SCSS CONSTANTS ###
################################
plugin.bootstrap_package.settings.scss {
    primary = #eb3e4a
    secondary = #013859
}

page.theme.footernavigation {
    navigationType = Directory
    navigationValue = 38
    enable = 1
}

